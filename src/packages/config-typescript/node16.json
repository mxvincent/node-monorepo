{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "NodeJS v16 typescript config",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "lib": [
      "es2021"
    ],
    "target": "es2021",
    "module": "commonjs",
    "moduleResolution": "node"
  }
}
